{
  "name": "@iso4217/json",
  "author": "Dima Parzhitsky <parzhitsky@gmail.com>",
  "version": "0.0.2",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parzh/iso4217.git"
  },
  "bugs": {
    "url": "https://github.com/parzh/iso4217/issues"
  },
  "homepage": "https://github.com/parzh/iso4217#readme",
  "files": [
    "/**/*.js",
    "!/**/*.type.js",
    "/**/*.js.map",
    "!/**/*.type.js.map",
    "/**/*.d.ts",
    "/iso4217.json",
    "/README.md"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "_ts-node": "export NODE_OPTIONS=\"--unhandled-rejections=strict\"; ts-node --project tsconfig.json",
    "build-iso4217-json": "npm run _ts-node -- ./build-iso4217-json.impl.ts",
    "build-iso4217-json-if-needed": "if [ ! -f \"./iso4217.json\" ]; then npm run build-iso4217-json; fi",
    "build:only": "tsc --project ./tsconfig.production.json",
    "prebuild": "npm run build-iso4217-json-if-needed",
    "build": "npm run build:only",
    "preversion": "test ${USING_CUSTOM_VERSIONING_SCRIPT:?''}",
    "v": "export USING_CUSTOM_VERSIONING_SCRIPT=$RANDOM; npm version --git-tag-version --sign-git-tag"
  },
  "devDependencies": {
    "@types/node": "16.9.6",
    "@types/xml2json": "0.11.4",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "xml2json": "0.11.2"
  },
  "peerDependencies": {
    "typescript": ">=1.5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
